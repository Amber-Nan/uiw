{
  "name": "uiw",
  "version": "3.10.10",
  "description": "A high quality UI Toolkit, A Component Library for React 16+.",
  "author": "Kenny Wong <wowohoo@qq.com>",
  "homepage": "https://uiwjs.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/uiwjs/uiw.git"
  },
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "files": [
    "lib",
    "src",
    "dist"
  ],
  "keywords": [
    "design",
    "uiw",
    "uiw-react",
    "react.js",
    "react",
    "react-component",
    "component",
    "components",
    "ui",
    "css",
    "uikit",
    "react-ui",
    "framework",
    "front-end",
    "frontend"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "bundle": "kkt build --bundle --no-checkRequiredFiles",
    "bundle:min": "GENERATE_SOURCEMAP=false kkt build --bundle --no-checkRequiredFiles --mini --no-emptyDir",
    "build": "npm run build:ts && npm run build:types && npm run bundle && npm run bundle:min",
    "build:ts": "tsbb build --target react",
    "build:types": "npm run types:esm && npm run types:cjs",
    "watch": "npm run watch:ts & npm run watch:types",
    "watch:ts": "tsbb watch --target react --env-name esm:dev --env-name cjs",
    "watch:types": "npm run types:esm -- --watch & npm run types:cjs -- --watch",
    "types:esm": "tsbb types --outDir lib/esm --target ESNEXT",
    "types:cjs": "tsbb types --outDir lib/cjs --target ESNEXT",
    "test": "tsbb test",
    "coverage": "tsbb test --coverage",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "@babel/runtime": ">=7.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
