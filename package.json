{
  "name": "uiw",
  "version": "2.0.0",
  "description": "A high quality UI Toolkit, A Component Library for React 16+.",
  "private": true,
  "sideEffects": true,
  "homepage": "https://uiwjs.github.io",
  "authors": [
    "Kenny Wang <wowohoo@qq.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uiwjs/uiw.git"
  },
  "scripts": {
    "start": "kkt start",
    "build": "kkt build",
    "test": "kkt test --env=jsdom",
    "deploy": "gh-pages -d dist -b master -r git@github.com:uiwjs/uiwjs.github.io.git",
    "deploy:pages": "gh-pages -d dist -b gh-pages",
    "released": "node script/released.js",
    "test:coverage": "kkt test --env=jsdom --coverage"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/packages/core/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/packages/core/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ]
  },
  "keywords": [
    "uiw",
    "uiwjs",
    "uiw-react",
    "react.js",
    "react",
    "component",
    "components",
    "ui",
    "framework"
  ],
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "dependencies": {
    "@babel/preset-react": "^7.0.0",
    "@babel/standalone": "^7.3.4",
    "@rematch/core": "^1.0.6",
    "@uiw/react-codemirror": "^1.0.19",
    "@uiw/reset.css": "^1.0.3",
    "classnames": "^2.2.6",
    "history": "^4.7.2",
    "prismjs": "^1.15.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-dynamic-loadable": "^1.1.2",
    "react-markdown": "^4.0.4",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "uiw": "file:./packages/core"
  },
  "devDependencies": {
    "@kkt/plugin-less": "^1.0.2",
    "kkt": "^3.5.0",
    "raw-loader": "^1.0.0",
    "react-test-renderer": "^16.7.0"
  },
  "author": "kenny wang <wowohoo@qq.com>",
  "license": "MIT"
}
