{
  "name": "uiw",
  "description": "A high quality UI Toolkit, A Component Library for React 16+.",
  "homepage": "https://uiwjs.github.io",
  "private": true,
  "version": "3.10.10",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna exec npm run build",
    "build:pkg": "lerna exec npm run build --scope @uiw/*",
    "coverage": "lerna exec npm run coverage --scope @uiw/react-button",
    "watch:uiw": "lerna run --scope uiw watch --stream",
    "watch:pkg": "lerna run --scope @uiw/* watch --stream",
    "version": "lerna version --no-changelog",
    "publish": "lerna publish from-package",
    "clean": "lerna clean",
    "compile-less": "compile-less",
    "kkt": "kkt",
    "tsbb": "tsbb",
    "lerna": "lerna"
  },
  "devDependencies": {
    "compile-less-cli": "^1.4.0",
    "kkt": "^5.8.0",
    "lerna": "^3.20.2",
    "tsbb": "^1.7.1"
  }
}
